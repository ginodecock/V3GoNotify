block head
	div 	
		table(border='3',width='900px')
			thead
				th Chip ID_
				th value 1
				th value 2
				th value 3
				th status
				th timestamp
			tbody
				each val in input
					tr
						td(contenteditable='true',style='width: 20%')=val.chipId
						td(contenteditable='true',style='width: 10%')=val.value1
						td(contenteditable='true',style='width: 10%')=val.value2
						td(contenteditable='true',style='width: 10%')=val.value3
						td(contenteditable='true',style='width: 10%')=val.status
						td(contenteditable='true',style='width: 40%')=val.timestamp	